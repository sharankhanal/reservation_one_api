{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://example.com/schemas/partner_one",
  "title": "Partner One Reservation",
  "type": "object",
  "properties": {
    "reservation_code": {
      "type": "string"
    },
    "start_date": {
      "type": "string"
    },
    "end_date": {
      "type": "string"
    },
    "nights": {
      "type": "integer"
    },
    "guests": {
      "type": "integer"
    },
    "adults": {
      "type": "integer"
    },
    "children": {
      "type": "integer"
    },
    "infants": {
      "type": "integer"
    },
    "status": {
      "type": "string"
    },
    "guest": {
      "type": "object",
      "properties": {
        "first_name": {
          "type": "string"
        },
        "last_name": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "email": {
          "type": "string"
        }
      },
      "required": [
        "first_name",
        "last_name",
        "phone",
        "email"
      ]
    },
    "currency": {
      "type": "string"
    },
    "payout_price": {
      "type": "string"
    },
    "security_price": {
      "type": "string"
    },
    "total_price": {
      "type": "string"
    }
  },
  "required": [
    "reservation_code",
    "start_date",
    "end_date",
    "nights",
    "guests",
    "adults",
    "children",
    "infants",
    "status",
    "guest",
    "currency",
    "payout_price",
    "security_price",
    "total_price"
  ]
}
